  ON TRUNC(sr.SEND_DT) = c.CAL_DT

WHERE sr.QUESTION_ID = 'Q1'
  AND FSCL_YR_NUM = 2018
  AND FSCL_QTR_IN_YR_NUM = 1

GROUP BY
  c.FSCL_YR_NUM
, c.FSCL_QTR_IN_YR_NUM</File>
